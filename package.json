  },
  "homepage": "https://github.com/maoxuena/snow-ui#readme",
  "dependencies": {
    "vue": "^2.6.12",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "chai": "^4.3.4",
    "chai-spies": "^1.0.0",
    "karma": "^6.3.1",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
